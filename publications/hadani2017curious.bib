@article{Vul2009,
	Abstract = {ABSTRACT---Functional magnetic resonance imaging (fMRI)$\backslash$nstudies of emotion, personality, and social cognition have$\backslash$ndrawn much attention in recent years, with high-profile$\backslash$nstudies frequently reporting extremely high (e.g., {\textgreater}.8) correlations$\backslash$nbetween brain activation and personalitymeasures.$\backslash$nWe show that these correlations are higher than should be$\backslash$nexpected given the (evidently limited) reliability of both fMRI$\backslash$nand personality measures. The high correlations are all the$\backslash$nmore puzzling because method sections rarely contain much$\backslash$ndetail about howthe correlationswere obtained.We surveyed$\backslash$nauthors of 55 articles that reported findings of this kind to$\backslash$ndetermine a few details on how these correlations were$\backslash$ncomputed. More than half acknowledged using a strategy$\backslash$nthat computes separate correlations for individual voxels and$\backslash$nreports means of only those voxels exceeding chosen thresholds.$\backslash$nWe show how this nonindependent analysis inflates$\backslash$ncorrelations while yielding reassuring-looking scattergrams.$\backslash$nThis analysis technique was used to obtain the vast majority$\backslash$nof the implausibly high correlations in our survey sample. In$\backslash$naddition, we argue that, in some cases, other analysis problems$\backslash$nlikely created entirely spurious correlations. We outline$\backslash$nhow the data from these studies could be reanalyzed$\backslash$nwith unbiased methods to provide accurate estimates of the$\backslash$ncorrelations in question and urge authors to perform such$\backslash$nreanalyses. The underlying problems described here appear$\backslash$nto be common in fMRI research of many kinds---not just in$\backslash$nstudies of emotion, personality, and social cognition.},
	Author = {Vul, Edward and Harris, Christine and Winkielman, Piotr and Pashler, Harold},
	Date-Added = {2019-05-28 12:28:11 -0700},
	Date-Modified = {2019-05-28 12:28:13 -0700},
	Doi = {10.1111/j.1745-6924.2009.01132.x},
	File = {:Users/ruthejane/Downloads/Users/ruthejane/Box Sync/pdfs/Vul Harris Winkielman Pashler 2009.pdf:pdf},
	Isbn = {1745-6916},
	Issn = {17456924},
	Journal = {Perspectives on Psychological Science},
	Number = {3},
	Pages = {319--324},
	Pmid = {9279245},
	Title = {{Reply to Comments on ``Puzzlingly High Correlations in fMRI Studies of Emotion, Personality, and Social Cognition''}},
	Url = {http://journals.sagepub.com/doi/10.1111/j.1745-6924.2009.01132.x},
	Volume = {4},
	Year = {2009},
	Bdsk-Url-1 = {http://journals.sagepub.com/doi/10.1111/j.1745-6924.2009.01132.x},
