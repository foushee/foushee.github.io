	Issn = {0388-0001},
	Journal = {Language Sciences},
	Month = {nov},
	Number = {6},
	Pages = {602--614},
	Publisher = {Pergamon},
	Title = {{On mother and other tongues: sociolinguistics, schools, and language ideology in northern India}},
