	Isbn = {9781781806715},
	Issn = {15516709},
	Journal = {Cognitive Science},
	Keywords = {Artifact categories,Efficient communication,Historical semantics,Semantic chaining,Semantic universals,Semantic variation},
	Number = {8},
	Pages = {2081--2094},
	Title = {{Historical Semantic Chaining and Efficient Communication: The Case of Container Names}},
