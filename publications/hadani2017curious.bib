	Issn = {1531-5320},
	Journal = {Psychonomic bulletin $\backslash${\&} review},
	Keywords = {frequency f,frequent word has a,known as zipf,language,obeying a power law,r,s law,statistics,that scales according to,the r th most,zipf},
	Number = {5},
