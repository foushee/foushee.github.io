	Issn = {00121630},
	Journal = {Developmental Psychobiology},
	Keywords = {Auditory function,Cochlear implantation,Critical period,Deaf children,Neurosensory development,Plasticity},
	Number = {3},
	Pages = {252--261},
	Pmid = {15772969},
	Title = {{Is there a critical period for cochlear implantation in congenitally deaf children? Analyses of hearing and speech perception performance after implantation}},
