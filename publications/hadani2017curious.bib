	Keywords = {age of acquisition,recognition,word,word frequency},
	Number = {September},
	Pages = {1--30},
	Pmid = {27028769},
	Title = {{Age of acquisition ratings score better on criterion validity than frequency trajectory or ratings 'corrected' for frequency.}},
