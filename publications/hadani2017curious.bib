@article{Ameel2008,
	Abstract = {Despite arguments for the relative ease of learning common noun meanings, semantic development continues well past the early years of language acquisition even for names of concrete objects. We studied evolution of the use of common nouns during later lexical development. Children aged 5-14 years and adults named common household objects and their naming patterns were compared. Children showed a gradual convergence to the adult categories through addition of new words to the vocabulary as well as extended reorganizations of existing categories. Different theories of early lexical development make competing proposals about the differences in featural knowledge that result in discrepancies from adult word use. To evaluate these theories with respect to later lexical development, we used features collected from adults and children to predict the naming patterns of the different age groups. Consistent with [Mervis, C. B. (1987). Child-basic object categories and early lexical development. In U. Neisser (Ed.), Concepts and conceptual development: ecological and intellectual factors in categorisation (pp. 201-233). Cambridge: Cambridge University Press], children gradually learned to attend to the adult feature sets and to assign the features the appropriate weights. A sorting task showed that, furthermore, evolution of general conceptual knowledge as well as word-specific knowledge contributes to the convergence. We discuss implications for developing a theory of later lexical development. {\textcopyright} 2007 Elsevier Inc. All rights reserved.},
	Author = {Ameel, Eef and Malt, Barbara and Storms, Gert},
	Date-Added = {2019-05-28 12:28:11 -0700},
	Date-Modified = {2019-05-28 12:28:14 -0700},
	Doi = {10.1016/j.jml.2007.01.006},
	File = {:Users/ruthejane/Box Sync/pdfs/ameel et al 2008.pdf:pdf},
	Isbn = {0749-596X},
	Issn = {0749596X},
	Journal = {Journal of Memory and Language},
	Keywords = {Feature-based theories,Later lexical development,Object naming},
	Pages = {262--285},
	Title = {{Object naming and later lexical development: From baby bottle to beer bottle}},
	Volume = {58},
	Year = {2008},
	Bdsk-Url-1 = {https://doi.org/10.1016/j.jml.2007.01.006}}

