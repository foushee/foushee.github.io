	Isbn = {1873-7838 (Electronic) 0010-0277 (Linking)},
	Issn = {18737838},
	Journal = {Cognition},
	Keywords = {Concreteness,Culturomics,Historical linguistics,Language evolution,Semantic bleaching},
	Pages = {87--92},
	Pmid = {26117487},
	Publisher = {Elsevier B.V.},
	Title = {{Recent evolution of learnability in American English from 1800 to 2000}},
