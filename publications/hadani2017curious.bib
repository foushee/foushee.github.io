@article{Izura2002,
	Abstract = {Four experiments explored the age of acquisition effects in the first and second languages of dominant Spanish-English bilinguals. In Experiment 1 (picture naming task) and Experiment 2 (lexical decision task), an age of acquisition effect was observed in a second language acquired after childhood as well as in the first language. The results suggest that age of acquisition effects reflect the order of word acquisition, which may in turn reflect the state of the lexical network when new words are learnt. The results do not support the idea that age of acquisition effects reflect differences between words learned during some critical period in childhood and words learned later in life. In Experiments 3 and 4, the age/order of second language acquisition affected lexical decision latencies regardless of the age at which translation equivalents were acquired in the first language, suggesting that the age of acquisition effect is linked to the acquisition of word forms rather than meanings.},
	Author = {Izura, Cristina and Ellis, Andrew W},
	Date-Added = {2019-05-28 12:28:11 -0700},
	Date-Modified = {2019-05-28 12:28:14 -0700},
	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Izura, Ellis - 2002 - Age of acquisition effects in word recognition and production in first and second languages.pdf:pdf},
	Isbn = {0211-2159},
	Issn = {0211-2159},
	Journal = {Psicologica},
	Pages = {245--281},
	Title = {{Age of acquisition effects in word recognition and production in first and second languages}},
	Year = {2002}}

@article{Welsh2011,
	Abstract = {This study examined developmental associations between growth in domain-general cognitive processes (working memory and attention control) and growth in domain-specific skills (emergent literacy and numeracy) across the pre-kindergarten year, and their relative contributions to kindergarten reading and math achievement. One hundred sixty-four Head Start children (44{\%} African American or Latino; 57{\%} female) were followed longitudinally. Path analyses revealed that working memory and attention control predicted growth in emergent literacy and numeracy skills during the pre-kindergarten year, and furthermore, that growth in these domain-general cognitive skills made unique contributions to the prediction of kindergarten math and reading achievement, controlling for growth in domain-specific skills. These findings extend research highlighting the importance of working memory and attention control for academic learning, demonstrating the effects in early childhood, prior to school entry. We discuss the implications of these findings for pre-kindergarten programs, particularly those designed to reduce the school readiness gaps associated with socio-economic disadvantage.},
	Annote = {Introduction

Documented and severe achievement gaps between middle-income and low-income children that expand and contribute to learning difficulties, educational attainment, and ultimate employment potential (Ryan, Fauth {\&} Brooks-Gunn, 2006)

What are the most effective strategies to reduce school readiness disparities? Should preschool programs focus on direct academic skill instruction (literacy, mathematics), as these are shown to be strong predictors of later success? Or should early educators focus on supporting the development of mental processes that underly effective approaches to learning, like working memory and attention control, which are both delayed in low-income children (Noble, McCandliss, {\&} Farah, 2007), and predict school adjustment and academic attainment (Blair {\&} Razza, 2007; Li-Grining, 2007; McClelland, Cameron, Connor, Farris, Jewkes, {\&} Morrison, 2007)?

Two central hypotheses: 

1) that growth in working memory and attention control would be associated concurrently with growth in emergent literacy and numeracy skills over the course of the pre-kindergarten year, and 

2) that growth of domain- general (working memory, attention control) and domain-specific (emergent literacy and numeracy) skills during the pre-kindergarten year would each make unique contributions to reading and math achievement in kindergarten

p. 2: executive function skills, particularly working memory and attention control, thus facilitate school readiness and early learning by supporting behavioral self-regulatory capacities and social competence (Blair, 2002; Hughes {\&} Ensor, 2007), and by fostering children's capacities to engage more effectively with teachers and peers in classroom learning activities (Hamre {\&} Pianta, 2005; Ladd, Birch, {\&} Buhs, 1999; Gathercole et al., 2008).

WORKING MEMORY
enables the retrieval of knowledge stored in long- term memory and its mental manipulation and application to foster the interpretation of novel information and the solution of problems

Evident relationship between working memory (WM) and both reading ability and math (addition, subtraction, word problems)

(Swanson et al, 2008; Swanson {\&} Jerman, 2007: WM supports development of problem-solving abilities, deficiencies underly reading disability

Passoulunghi et al (2006): working memory at school entry predictive of math task performance and later achievement

ATTENTION CONTROL
includes the capacity to focus and flexibly shift attention, as well as to ignore irrelevant stimuli and inhibit prepotent responding to stimuli, in order to respond to task demands (Blair {\&} Diamond, 2008)

Attention control associated with math and reading success

Method

Participants
164 children in their prekindergarten year in the control group of the Head Start REDI project (i.e., did not receive enhanced curriculum) 

Assessments 
Conducted at 3 time points
1) the beginning of the pre-kindergarten year, as soon as children had acclimated to the classroom setting, 
2) the end of the pre- kindergarten year, and 
3) the end of kindergarten.

Domain-specific Skills

Emergent Literacy Skills
Composite score derived from average of scores on 3 Measures, from the Test of Preschool Early Literacy (TOPEL)
Print Knowledge
identify and name letters or words
Blending
assessing phonological processing; combine parts of words ("b" and "air" to get "bear")
Elision
deconstruct compound words (point to picture corresponding to "snowshoe" without "snow")

Reading Achievement
Assessed at end of kindergarten year
Composite Score derived from scores on four measures:
1) Letter-Word Identification (from the Woodcock Johnson III: Test of Achievement)
decoding skills
2) Story Recall (from the Woodcock Johnson III: Test of Achievement)
memory for story content
3) Sight Word Efficiency from the Test of Word Reading Efficiency
speed identifying printed words 
4) Phonemic Decoding Efficiency (from the TOWRE)
speed accurately sounding out non-words

Emergent numeracy skills {\&} Math achievement
Assessment:
Applied Problems from the Woodcock -- Johnson III: Tests of Achievement
understanding of numbers and quantities (showing two fingers, counting objects, and adding or subtracting small numbers)

Domain-General Cognitive Abilities
Composite EF score derived from average of scores on three tasks:
1) Backward Word Span
assesses working memory: children are asked to repeat a list of words read aloud in reverse order

2) Peg Tapping 
assesses inhibitory attention control: children must tap a wooden dowel twice when the experimenter taps once, and to tap once when the experimenter taps twice, contrart to natural impulse to imitate

3) Dimensional Change Card Sort
assesses attention set-shifting abilities: children are taught to sort cards along one dimension, e.g., color, and then asked to sort the cards along the other dimension after 12 trials

Language skills covariate
Given that language skills will mediate any learning in preschool, a composite score derived from scores on three assessments:
1) Expressive One-Word Picture Vocabulary Test
vocabulary: asked to give one word to describe pictures
2) Grammatical Understanding (from the Test of Language Development -- Revised)
syntax comprehension: children hear a sentence and choose the picture that best matches it
3) Sentence Imitation (from the TOLDP:III)
syntax expression: children repeat increasingly complex sentences read by experimenter

Results
High correlations between all cognitive skill variables, and stability across the pre-K year

Pre-K literacy and mathematics skills significantly predict reading and math achievement, respectively

Pre-K working memory and attention control measures predicted kinder reading and math achievement

Initial levels of executive functions predicted growth in emergent literacy and numeracy skills between the beginning and end of the pre-kindergarten year, with initial levels of those skills (and initial levels of language skills) accounted for.

Growth in emergent literacy skills during Head Start predicted kindergarten reading achievement ($\beta$ = .25). Growth in EF had an additional, unique influence on kindergarten reading achievement

*a reciprocal relation also emerged, such that initial levels of emergent numeracy skills predicted growth in executive functions during the pre-kindergarten year ($\beta$ = .21), with initial levels of executive functions controlled

initial levels and growth of emergent numeracy skills made unique contributions to kinder math achievement, controlling for the effect of growth those skills during the pre-K period

growth in EF skills in pre-K made unique contributions to kinder math achievement

Conclusion
For reading and math, both domain-specific and domain-general cognitive skills made unique contributions to children's kindergarten reading achievement, suggesting that the "early development of working memory and attention control provide an important foundation for domain-specific academic learning." Given that early math ability is a reliable predictor of long-term academic achievement, the reciprocal relationship found between emergent math skills and EF skills is especially interesting. Pre-K math activities, in particulary, may place demands on working memory and attention control that relate to growth in those domain-general skills. Emergent literacy skills, on the other hand, were not predictive of domain-general EF skills, suggesting they represent, in the words of the authors, "acquired, acculturated knowledge, or crystallized abilities," unrelated to the growth of EF abilities. Finally and importantly, children growing up with socioeconomic disadvantage evidence delays in EF skill development. Developmental research suggests a relationship between those skills and aspects of the caretaking environment (sensitive-responsive caregiving, support for sustained joint attention, and effective scaffolding of early emotion regulation: Cicchetti, 2002; Goldsmith {\&} Rogoff, 1997; Lengua, Honorado {\&} Bush, 2007) that may be diminished in households affected by poverty. Extreme adversity and traumatic events are also, relatedly, associated with deficits in EF skills, (Cicchetti, 2002; O'Connor, Rutter, Beckett, et al., 2000). 

Discussion of interventional approaches},
	Author = {Welsh, Janet a and Nix, Robert L and Blair, Clancy and Bierman, Karen L and Nelson, Keith E},
	Date-Added = {2019-05-28 12:28:11 -0700},
	Date-Modified = {2019-05-28 12:28:14 -0700},
	Doi = {10.1037/a0016738.The},
	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Welsh et al. - 2011 - NIH Public Access.pdf:pdf},
	Number = {1},
	Pages = {43--53},
	Title = {{NIH Public Access}},
	Volume = {102},
	Year = {2011},
	Bdsk-Url-1 = {https://doi.org/10.1037/a0016738.The}}

@article{Lam2009,
	Abstract = {In this study we examined the relationship between teacher and student intrinsic motivation in project-based learning. The participants were 126 Hong Kong secondary school teachers and their 631 students who completed evaluation questionnaires after a semester-long project-based learning program. Both teachers and students were asked to indicate their motivation in the program, and students were also asked to report the instructional support they received from their teachers. The results of hierarchical linear modeling analyses showed that teacher intrinsic motivation predicted student intrinsic motivation directly as well as indirectly through the mediation of instructional support. When teachers reported higher intrinsic motivation in the program, their students tended to perceive receiving more support from them and to report higher intrinsic motivation in the learning experience.},
