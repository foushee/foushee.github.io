	Keywords = {Exploration,Imitation,Model-based bias,Social learning,Trust,Uncertainty bias},
	Pages = {272--284},
	Pmid = {27371768},
	Title = {{``Model age-based'' and ``copy when uncertain'' biases in children's social learning of a novel task}},
	Volume = {150},
