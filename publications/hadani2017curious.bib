	Journal = {Collabra: Psychology},
	Keywords = {consumers must evaluate the,consumers of new scientific,in any market,in science,on,open science,products and decide their,quality of,replicability,scientific integrity,their evaluation,transparency,willingness to pay based},
	Number = {1},
	Pages = {1},
	Pmid = {1641842234},
	Title = {{Quality Uncertainty Erodes Trust in Science}},
	Volume = {3},
	Year = {2017},
