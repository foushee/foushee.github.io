	Issn = {1363755X},
	Journal = {Developmental Science},
	Keywords = {cross-cultural, overhearing, input, naturalistic, longitudinal, mayan, language acquisition, vocabulary},
	Number = {5},
	Pages = {659--673},
	Title = {{Language input and acquisition in a {M}ayan village: How important is directed speech?}},
	Url = {http://doi.wiley.com/10.1111/j.1467-7687.2012.01168.x},
	Volume = {15},
