	Journal = {Journal of Educational Psychology},
	Keywords = {complex domains,experimentation,explicit instruction,however,inquiry in science,involves processes that are,like reasoning in other,of error,reasoning in science,scaling up,science education,science has developed a,susceptible to various forms,unlike many other domains},
	Number = {1},
	Pages = {90--101},
	Title = {{Learning the control of variables strategy in higher and lower achieving classrooms: Contributions of explicit instruction and experimentation.}},
