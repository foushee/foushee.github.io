	File = {:Users/ruthejane/Box Sync/pdfs/Henderson,Sabbagh,Woodward Cognition 2012.pdf:pdf},
	Isbn = {0010-0277},
	Issn = {00100277},
	Journal = {Cognition},
	Keywords = {Conventionality,Preschoolers,Relevance,Selective learning,Word learning},
	Number = {2},
