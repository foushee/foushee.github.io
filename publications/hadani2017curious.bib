	Issn = {0027-8424},
	Journal = {Proceedings of the National Academy of Sciences},
	Number = {37},
	Pages = {15145--15150},
	Pmid = {23980148},
	Title = {{Learning-induced neural plasticity of speech processing before birth}},
	Url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1302159110},
	Volume = {110},
