	Isbn = {0022-1031},
	Issn = {00221031},
	Journal = {Journal of Experimental Social Psychology},
	Keywords = {Automatic and controlled processes,Egocentric biases,Egocentrism,Judgmental correction,Perspective taking,Theory of mind},
	Number = {6},
