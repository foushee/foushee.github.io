	Doi = {10.1073/pnas.1014076108},
	File = {::},
	Issn = {1091-6490},
	Journal = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {Autistic Disorder,Autistic Disorder: psychology,Child,Child Behavior,Child Behavior: psychology,Cognition,Cognition: physiology,Exploratory Behavior,Exploratory Behavior: physiology,Female,Great Britain,Humans,Male,Neuropsychological Tests,Space Perception,Space Perception: physiology,Spatial Behavior,Spatial Behavior: physiology},
	Month = {jan},
	Number = {1},
	Pages = {421--6},
	Pmid = {21173235},
	Title = {{Children with autism are neither systematic nor optimal foragers.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3017188{\&}tool=pmcentrez{\&}rendertype=abstract},
	Volume = {108},
