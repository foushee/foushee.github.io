	Keywords = {Adaptation, Psychological,Attitude,Culture,Humans,Individuality,Interpersonal Relations,Models, Theoretical,Morals,Personal Autonomy,Personal Satisfaction,Power (Psychology),Psychological Theory,Psychology, Social,Self Concept,Social Behavior,Social Class,Social Environment,Socioeconomic Factors},
	Month = {jul},
	Number = {3},
	Pages = {546--72},
	Pmid = {22775498},
	Title = {{Social class, solipsism, and contextualism: how the rich are different from the poor.}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/22775498},
	Volume = {119},
	Year = {2012},
