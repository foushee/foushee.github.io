	Doi = {10.1017/S0305000915000148},
	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Mastin, Vogt - 2016 - Infant engagement and early vocabulary development A naturalistic observation study of Mozambican infants from 11.pdf:pdf},
