	Journal = {Journal of experimental child psychology},
	Keywords = {child,female,human,language,male,multilingualism,social behavior,speech perception},
	Title = {{Bilingual children's social preferences hinge on accent}},
	Year = {2017},
	Bdsk-Url-1 = {https://doi.org/10.1016/j.jecp.2017.07.005}}

