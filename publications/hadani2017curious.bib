	Keywords = {Adult,Brain Mapping,Cerebral Cortex,Cerebral Cortex: physiology,Concept Formation,Humans,Magnetic Resonance Imaging,Male,Reference Values,Semantics,Verbal Behavior},
	Month = {dec},
	Number = {6},
	Pages = {1210--24},
	Pmid = {23259955},
