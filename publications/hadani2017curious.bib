	Keywords = {Lexical processing,activation,age of acquisition,frequency,naive discrimination learning,prior availability,subtitles},
	Title = {{Frequency in lexical processing}},
	Year = {2016},
	Bdsk-Url-1 = {https://doi.org/10.1080/02687038.2016.1147767}}

