	Isbn = {0024-3841},
	Issn = {00243841},
	Journal = {Lingua},
	Keywords = {Breakpoint regression,CHILDES database,Child-directed speech,Language development,Mean length of utterance,Speaking rate},
	Number = {8},
	Pages = {841--857},
	Publisher = {Elsevier B.V.},
	Title = {{Nonlinear development of speaking rate in child-directed speech}},
	Url = {http://dx.doi.org/10.1016/j.lingua.2012.02.005},
	Volume = {122},
	Year = {2012},
