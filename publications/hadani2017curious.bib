	File = {:Users/ruthejane/Box Sync/pdfs/framing.pdf:pdf},
	Isbn = {1467-9280 (Electronic)$\backslash$r0956-7976 (Linking)},
	Issn = {14679280},
	Journal = {Psychological Science},
	Keywords = {context effects,judgment and decision making,open data,open materials,scarcity},
	Number = {4},
	Pages = {402--412},
