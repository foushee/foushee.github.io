@article{Truong2012,
	Abstract = {In this paper, we investigate prosodic alignment in task-based conversations.$\backslash$nWe use the HCRC Map Task Corpus and investigate how familiarity affects$\backslash$nprosodic alignment and how task success is related to prosodic alignment.$\backslash$nA variety of existing alignment measures is used and applied to our$\backslash$ndata. In particular, a windowed cross-correlation procedure, that$\backslash$nhas been used previously in visual behavior research, is applied$\backslash$nto prosodic features. In addition, we address the issue of how to$\backslash$nseparate genuine observed alignment from alignment that is a result$\backslash$nfrom random coincidental behavior. Using these measures, we find$\backslash$nsome indications of prosodic convergence and synchrony in the map$\backslash$ntask conversations. Alignment tendencies are strongest for intensity,$\backslash$nand familiarity seems to play a role in convergence. Finally, weak$\backslash$nevidence was found for a correlation between prosodic alignment measures$\backslash$nand task success.},
	Author = {Truong, Drs. Khiet P and Heylen, Dr. Dirk},
