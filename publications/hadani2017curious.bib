@article{Plantinga2018,
	Abstract = {Previous research shows that the wealthy neglect opportunity costs (Frederick, Novemsky, Wang, Dhar, {\&} Nowlis, 2009). When the not-buying option is framed as ``keeping the money for other purchases'' participants are less willing to purchase, suggesting that they normally fail to consider forgone opportunities. Other research suggests that the poor should be more likely to spontaneously consider opportunity costs (Shah, Shafir, {\&} Mullainathan, 2015; Spiller, 2011). This implies that only the rich should neglect opportunity costs. We tested this hypothesis using Frederick et al.'s (2009) paradigm. Five high-powered experiments (total N = 2325) show that both high- and low-income participants neglect opportunity costs. The experiments used different products (both material and experiential) with both high and low prices ({\$}8.50--{\$}249.99), and different manipulations of opportunity costs consideration. Meta-analyses show a strong effect of condition on choices (demonstrating opportunity cost neglect), and an effect of income on choice (demonstrating sufficient variability in income). However, we do not find the suggested interaction effect of condition and income. In sum, the poor seem to be at least as likely to neglect opportunity costs as the rich. Keywords:},
	Author = {Plantinga, Arnoud and Krijnen, Job M.T. and Zeelenberg, Marcel and Breugelmans, Seger M.},
