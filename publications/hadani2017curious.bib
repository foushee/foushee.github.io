	Doi = {10.1073/pnas.0910967107},
	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Beilock et al. - 2010 - Female teachers' math anxiety affects girls' math achievement.pdf:pdf},
	Isbn = {1091-6490 (Electronic)$\backslash$r0027-8424 (Linking)},
	Issn = {1091-6490},
	Journal = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {Anxiety,Child,Faculty,Fear,Female,Humans,Male,Mathematics,Mathematics: education,Self-Assessment,Sex Characteristics,Students,Students: psychology,United States},
	Number = {5},
	Pages = {1860--3},
