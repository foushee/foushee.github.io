@article{Spalek2014,
	Abstract = {Bilingualism research has established language non-selective lexical access in comprehension. However, the evidence for such an effect in production remains sparse and its neural time-course has not yet been investigated. We demonstrate that German-English bilinguals performing a simple picture-naming task exclusively in English spontaneously access the phonological form of -unproduced- German words. Participants were asked to produce English adjective-noun sequences describing the colour and identity of familiar objects presented as line drawings. We associated adjective and picture names such that their onsets phonologically overlapped in English (e.g., green goat), in German through translation (e.g., blue flower - 'blaue Blume'), or in neither language. As expected, phonological priming in English modulated event-related brain potentials over the frontocentral scalp region from around 440. ms after picture onset. Phonological priming in German was detectable even earlier, from 300. ms, even though German was never produced and in the absence of an interaction between language and phonological repetition priming at any point in time. Overall, these results establish the existence of non-selective access to phonological representations of the two languages in the domain of speech production. ?? 2014.},
	Annote = {"Are bilingual speakers' lexicons always both active during speech production, even when speaking the non-dominant/non-native language?"
The authors crossed two within-subjects variables in their stimuli: phonological overlap and language of the overlap. Subjects were asked to name pictures with the appropriate adjective-noun pair in their L2, English. The adjective and noun either shared an onset in German (their L1), shared an onset in English, or did not in either. The authors collected ERP and reaction time data. They expected that phoneme repetition in L2 would decrease ERP amplitudes around 350 ms after stimulus presentation, and were interested in the time course of the effect of phoneme repetition on ERP amplitudes (if it existed) when the repetition was in L1 versus L2. The authors seemed also to expect that reaction times in the phoneme repetition conditions of each language might be faster than those of controls.
The authors found no behavioral evidence for a phonological priming effect when the phonological overlap was in speakers' L1 and production was in their L2: reaction times for production of adjective-noun pairs with and without overlapping onsets was different only when that overlap was in L2 (the language of production). They did, however, find somewhat weak evidence for non-language-specific phoneme repetition priming for both languages in the ERP data: phoneme repetition was associated with decreased ERP amplitudes for both languages, but a significant difference between repetition and control was seen 150 ms earlier for L1 than for L2. The authors take their findings to imply that phonological segments, in addition to lexical alternatives, for both languages are activated in the L2 language production of a bilingual speaker. They may also say something about the relative time course of L1 versus L2 activation, with L1 apparently more rapid or immediate.},
	Author = {Spalek, Katharina and Hoshino, Noriko and Wu, Yan Jing and Damian, Markus and Thierry, Guillaume},
	Date-Added = {2019-05-28 12:28:11 -0700},
	Date-Modified = {2019-05-28 12:28:13 -0700},
	Doi = {10.1016/j.cognition.2014.06.016},
	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Spalek et al. - 2014 - Speaking two languages at once Unconscious native word form access in second language production.pdf:pdf},
	Isbn = {0010-0277},
	Issn = {18737838},
	Journal = {Cognition},
	Keywords = {Bilingualism,Event-related potentials,Lexical access,Phonological co-activation,Speech},
	Number = {1},
	Pages = {226--231},
	Pmid = {25058411},
	Publisher = {Elsevier B.V.},
	Title = {{Speaking two languages at once: Unconscious native word form access in second language production}},
	Url = {http://dx.doi.org/10.1016/j.cognition.2014.06.016},
	Volume = {133},
	Year = {2014},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.cognition.2014.06.016}}

