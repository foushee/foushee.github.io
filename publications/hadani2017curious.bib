	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Kidd, Palmeri, Aslin - 2013 - Rational snacking Young children's decision-making on the marshmallow task is moderated by beliefs about e.pdf:pdf},
	Isbn = {0010-0277},
	Issn = {00100277},
	Journal = {Cognition},
	Keywords = {Child learning,Decision-making,Delay of gratification,Marshmallow task,Rational analysis},
	Number = {1},
	Pages = {109--114},
	Pmid = {23063236},
	Publisher = {Elsevier B.V.},
	Title = {{Rational snacking: Young children's decision-making on the marshmallow task is moderated by beliefs about environmental reliability}},
