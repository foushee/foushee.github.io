@article{Maloney2015,
	Abstract = {A large field study of children in first and second grade explored how parents' anxiety about math relates to their children's math achievement. The goal of the study was to better understand why some students perform worse in math than others. We tested whether parents' math anxiety predicts their children's math achievement across the school year. We found that when parents are more math anxious, their children learn significantly less math over the school year and have more math anxiety by the school year's end---but only if math-anxious parents report providing frequent help with math homework. Notably, when parents reported helping with math homework less often, children's math achievement and attitudes were not related to parents' math anxiety. Parents' math anxiety did not predict children's reading achievement, which suggests that the effects of parents' math anxiety are specific to children's math achievement. These findings provide evidence of a mechanism for intergenerational transmission of low math achievement and high math anxiety. (PsycINFO Database Record (c) 2015 APA, all rights reserved). (journal abstract)},
	Author = {Maloney, Erin A. and Ramirez, Gerardo and Gunderson, Elizabeth a and Levine, Susan C. and Beilock, Sian L.},
	Date-Added = {2019-05-28 12:28:11 -0700},
	Date-Modified = {2019-05-28 12:28:14 -0700},
	Doi = {10.1177/0956797615592630},
	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Maloney et al. - 2015 - Intergenerational effects of parents' math anxiety on children's math achievement and anxiety.pdf:pdf},
	Isbn = {0956-7976},
	Issn = {0956-7976, 1467-9280},
	Journal = {Psychological Science},
	Keywords = {Academic Achievement,Cognitive Development,Emotional Development,Mathematics Achievement,Mathematics Anxiety,Parents,Transgenerational Patterns,academic achievement,cognitive development,educational psychology,emotional development,mathematics achievement},
	Number = {9},
	Pages = {1480--1488},
	Pmid = {26253552},
	Title = {{Intergenerational effects of parents' math anxiety on children's math achievement and anxiety.}},
	Url = {erinmaloney@uchicago.edu,{\%}5CnBeilock@uchicago.edu},
	Volume = {26},
	Year = {2015},
	Bdsk-Url-1 = {https://doi.org/10.1177/0956797615592630}}

@article{Tolins2016,
	Abstract = {Observing others in conversation is a common format for comprehending language, yet little work has been done to understand dialog comprehension. We tested whether overhearers use addressee backchannels as predictive cues for how to integrate information across speaker turns during comprehension of spontaneously produced collaborative narration. In Experiment 1, words that followed specific backchannels (e.g., really, oh) were recognized more slowly than words that followed either generic backchannels (e.g., uh huh, mhm) or pauses. In Experiment 2, we found that when the turn after the backchannel was a continuation of the narrative, specific backchannels prompted the fastest verification of prior information. When the turn after was an elaboration, they prompted the slowest, indicating that overhearers took specific backchannels as cues to integrate preceding talk with subsequent talk. These findings demonstrate that overhearers capitalize on the predictive relationship between backchannels and the development of speakers' talk, coordinating information across conversational roles. Copyright {\textcopyright} 2015 Cognitive Science Society, Inc.},
	Author = {Tolins, Jackson and {Fox Tree}, Jean E.},
	Date-Added = {2019-05-28 12:28:11 -0700},
	Date-Modified = {2019-05-28 12:28:14 -0700},
	Doi = {10.1111/cogs.12278},
	File = {:Users/ruthejane/Downloads/Users/ruthejane/Box Sync/pdfs/Tolins{\_}et{\_}al-2016-Cognitive{\_}Science.pdf:pdf},
	Issn = {15516709},
	Journal = {Cognitive Science},
	Keywords = {Backchannels,Comprehension,Dialog,Narrative,Overhearers},
	Number = {6},
	Pages = {1412--1434},
	Pmid = {26331673},
	Title = {{Overhearers Use Addressee Backchannels in Dialog Comprehension}},
	Volume = {40},
