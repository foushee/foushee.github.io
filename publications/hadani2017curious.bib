@article{Pietraszewski2014,
	Abstract = {The present studies explore whether accent behaves like a byproduct of coalitional categorization, or like a dedicated dimension of social categorization. An experimental manipulation which has previously been shown to reduce coalitional byproducts, such as race, but not affect dedicated dimensions, such as sex and age, was used to test between these two possibilities. Accent behaved like a dedicated dimension, remaining unaffected by the same coalitional manipulation that reduces categorization by race. A second study verified that the exact same coalitional manipulation used with accent in fact reduces categorization by race. These results suggest that accent is not a byproduct of coalitional psychology, unlike race. Implications for the differing proximate psychologies underlying race and accent, and for the construct group, are discussed. {\textcopyright} 2014 Elsevier Inc.},
	Author = {Pietraszewski, David and Schwartz, Alex},
	Date-Added = {2019-05-28 12:28:11 -0700},
	Date-Modified = {2019-05-28 12:28:13 -0700},
	Doi = {10.1016/j.evolhumbehav.2013.09.005},
	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Pietraszewski, Schwartz - 2014 - Evidence that accent is a dedicated dimension of social categorization, not a byproduct of coalitional.pdf:pdf},
	Isbn = {10905138},
	Issn = {10905138},
	Journal = {Evolution and Human Behavior},
	Keywords = {Accent,Coalitional psychology,Cognition,Race,Social categories},
	Title = {{Evidence that accent is a dedicated dimension of social categorization, not a byproduct of coalitional categorization}},
	Year = {2014},
	Bdsk-Url-1 = {https://doi.org/10.1016/j.evolhumbehav.2013.09.005}}

