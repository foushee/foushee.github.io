@article{Srinivasan,
	Abstract = {In three studies, we explored what the flexible use of a word like hammer, to label an object (``She bought a hammer'') and a functional use of that object (``She hammered the nail''), reveal about the structure of word meanings and the locus of creativity in language. We tested the idea that such uses of words are supported by complex, generative structures that specify the meaning of a noun (e.g., a hammer) and its corresponding verb (e.g., to hammer). In support of this idea, we showed that 4- and 5-year-olds spontaneously expect a novel label for an action to also label the instrument involved in that action, in the absence of any evidence that it can. To show this, we taught children a novel word for an action (e.g., that dax labeled hammering a nail). We then presented children with pictures of the instrument (e.g., a hammer) and patient of the event (e.g., a nail), and asked them to indicate the referent of a second word (e.g., ``Show me the kiv!''). Children systematically chose the patient, suggesting that they expected that the first word, which had labeled the action, would also label the instrument, blocking the second word from labeling it. Children displayed this tendency both when the first novel word had a familiar meaning, and when it had an entirely novel meaning. Together, our studies suggest that the noun and verb forms of words like hammer do not correspond to unrelated lexical items, but are instead related by generative structures that allow children to anticipate the flexibility of new words.},
	Author = {Srinivasan, Mahesh and Barner, David},
	Date-Added = {2019-05-28 12:28:11 -0700},
	Date-Modified = {2019-05-28 12:28:14 -0700},
	File = {::},
	Keywords = {class-extension rules,language development,lexical semantics,polysemy,syntactic development},
	Title = {{Daxing with a dax: Generative structures support noun-verb flexibility in the absence of ostensive learning}}}

@article{Collins1975,
	Abstract = {This paper presents a spreading-activation theory of human semantic pro- cessing, which can be applied to a wide range of recent experimental re- sults. The theory is based on Quillian's theory of semantic memory search and semantic preparation, or priming. In conjunction with this, several of the miscondeptions concerning Qullian's theory are discussed. A number of additional assumptions are proposed for his theory in order to apply it to recent experiments. The present paper shows how the extended theory can account for results of several production experiments by Loftus, Juola and Atkinson's multiple-category experiment, Conrad's sentence-verification experiments, and several categorization experiments on the effect of semantic relatedness and typicality by Holyoak and Glass, Rips, Shoben, and Smith, and Rosch. The paper also provides a critique of the Smith, Shoben, and Rips model for categorization judgments.},
