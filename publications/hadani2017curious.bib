	Isbn = {9780444537027},
	Issn = {00796123},
	Journal = {Progress in Brain Research},
	Keywords = {Cognitive performance benefits,Nap length,Naps,Process O,Sleep homeostasis,Sleep inertia},
	Number = {C},
	Pages = {155--166},
