	Isbn = {1554-3528},
	Issn = {15543528},
	Journal = {Behavior Research Methods},
	Keywords = {Age-of-acquisition,AoA,Estimates,Norms,Ratings},
	Title = {{Rated age-of-acquisition norms for over 3,200 German words}},
