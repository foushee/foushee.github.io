@article{Arnold2008,
	Abstract = {This paper reviews research on the production of definite referring expressions from both addressee-oriented and speaker-oriented perspectives. The focus will be on the choice between more- and less-attenuated expressions, for example between pronouns and names or descriptions (it vs. the block), between more or less explicit descriptions (the red block vs. the block) or between acoustically attenuated and prominent versions of a particular expression. Although this paper describes research on nominal referring expressions, event reference is sensitive to many of the same issues (see Kehler {\&} Ward, 2004). This paper also focuses on spoken language. Speaking is the most basic form of language use, and requires on-line integration of information about the context and addressee's knowledge in a more dynamic way than writing does. It therefore presents a good testing ground of the interplay between speaker-oriented and addressee-oriented processes during reference production.},
	Annote = {This paper reviews research on the production of definite referring expressions from both addressee-oriented and speaker-oriented perspectives. The focus will be on the choice between more- and less-attenuated expressions, for example between pronouns and names or descriptions (it vs. the block), between more or less explicit descriptions (the red block vs. the block) or between acoustically attenuated and prominent versions of a particular expression. Although this paper describes research on nominal referring expressions, event reference is sensitive to many of the same issues (see Kehler {\&} Ward, 2004). This paper also focuses on spoken language. Speaking is the most basic form of language use, and requires on-line integration of information about the context and addressee's knowledge in a more dynamic way than writing does. It therefore presents a good testing ground of the interplay between speaker-oriented and addressee-oriented processes during reference production.

The Expectancy hypothesis suggests that during language comprehension, listeners focus their attention on discourse entities in proportion to their estimation of the likelihood that the entity will be mentioned. This has implications for understanding the role of audience design in reference production, because it provides a mechanism for communicative facilitation even when the producer did not explicitly intend it. The Expectancy hypothesis emerges out of the finding that the above- mentioned textual factors share an additional property: they are all correlated with a relatively higher likelihood of subsequent mention (Arnold, 1998; see also Arnold, Brown-Schmidt, {\&} Trueswell, 2007b; Arnold {\&} Tanenhaus, in press). For example, speakers and writers are more likely to refer back to something that was mentioned in subject position than things mentioned in non-subject position (Arnold, 1998), even when considering only utterances with more than one nominal argument or adjunct (Arnold, 1999). Similarly, speakers are more likely to refer to things in goal role than source role, things in parallel syntactic position, things in the focus position of clefts, and things that are recently mentioned (Arnold, 1998, 2001).
$\backslash$ldots Through experience with language, people learn that some things (e.g., those mentioned as subjects) are more likely to be continued in the discourse than others. By focusing their attention probabilistically on the more likely referents, they can facilitate comprehension of references to those entities$\backslash$ldots discourse entities vary in accessibility in a gradient way. It also predicts that referents should be perceived as more accessible in situations where textual or non-textual factors indicate that there is a higher likelihood that it will be mentioned, thus facilitating reference comprehension. The expectancy of discourse entities is likely to be represented in the nonlinguistic model of the situation, where expected entities have a higher level of activation (Arnold, 1998). That is, what is relevant is the likelihood of a referential event, not the likelihood of particular linguistic forms of reference. In this sense the Expectancy hypothesis contrasts with another model of linguistic predictability, Pick- ering and Garrod's (2007) production emulator, which models the predict- ability of linguistic forms and structures during lexical and syntactic processing. Thus, the Expectancy hypothesis predicts that reference to expected entities should be facilitated in comprehension, while the produc- tion emulator in its current form makes predictions about linguistic forms and structures. According to the Expectancy hypothesis, the calculation of expectancy
occurs for the comprehender, who uses referential expectancy as a proxy for the speaker's focus of attention. In other words, the way the speaker phrases things provides useful information to the listener about what the speaker is likely to keep talking about. A similar idea is represented in Centering theory's list of forward-looking centres, which constitute the set of likely topics in the immediately following discourse (e.g., Brennan, 1995; Grosz et al., 1995). But this just raises a new question about addressee design: why do speakers mark accessibility through their choices in phrasing?},
	Author = {Arnold, Jennifer E.},
	Date-Added = {2019-05-28 12:28:11 -0700},
	Date-Modified = {2019-05-28 12:28:13 -0700},
	Doi = {10.1080/01690960801920099},
	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Arnold - 2008 - Reference production Production-internal and addressee-oriented processes.pdf:pdf},
	Isbn = {0169-0965},
	Issn = {0169-0965},
	Journal = {Language and Cognitive Processes},
	Number = {4},
	Pages = {495--527},
	Title = {{Reference production: Production-internal and addressee-oriented processes}},
	Url = {http://www.tandfonline.com/doi/abs/10.1080/01690960801920099},
	Volume = {23},
	Year = {2008},
	Bdsk-Url-1 = {http://www.tandfonline.com/doi/abs/10.1080/01690960801920099},
	Bdsk-Url-2 = {https://doi.org/10.1080/01690960801920099}}

