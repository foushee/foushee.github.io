@article{Everett2005,
	Abstract = {The Piraha˜ language challenges simplistic application of Hock- $\backslash$nett's nearly universally accepted design features of human lan- $\backslash$nguage by showing that some of these features (interchangeability, $\backslash$ndisplacement, and productivity) may be culturally constrained. In $\backslash$nparticular, Piraha˜ culture constrains communication to nonab- $\backslash$nstract subjects which fall within the immediate experience of in- $\backslash$nterlocutors. This constraint explains a number of very surprising $\backslash$nfeatures of Piraha˜ grammar and culture: the absence of numbers $\backslash$nof any kind or a concept of counting and of any terms for quanti- $\backslash$nfication, the absence of color terms, the absence of embedding, $\backslash$nthe simplest pronoun inventory known, the absence of ``relative $\backslash$ntenses,'' the simplest kinship system yet documented, the ab- $\backslash$nsence of creation myths and fiction, the absence of any individ- $\backslash$nual or collective memory of more than two generations past, the $\backslash$nabsence of drawing or other art and one of the simplest material $\backslash$ncultures documented, and the fact that the Piraha˜ are monolin- $\backslash$ngual after more than 200 years of regular contact with Brazilians $\backslash$nand the Tupi-Guarani-speaking Kawahiv.},
	Author = {Everett, Daniel L.},
