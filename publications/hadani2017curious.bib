	Journal = {PLoS ONE},
	Keywords = {interaction, CDS, child-directed interaction, imitation, learning},
	Number = {10},
	Pmid = {25333623},
	Title = {{Why do child-directed interactions support imitative learning in young children?}},
	Volume = {9},
