	File = {:Users/ruthejane/Box Sync/pdfs/LibertusEtAl2015.pdf:pdf},
	Isbn = {1524-8372},
	Issn = {15327647},
	Journal = {Journal of Cognition and Development},
	Number = {3},
	Pages = {442--454},
	Title = {{A Developmental Vocabulary Assessment for Parents (DVAP): Validating Parental Report of Vocabulary Size in 2- to 7-Year-Old Children}},
