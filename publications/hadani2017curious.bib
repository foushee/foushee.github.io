	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Freer, Roy, Tenenbaum - 2012 - Towards common-sense reasoning via conditional simulation legacies of Turing in Artificial Intelligence.pdf:pdf},
	Isbn = {1-107-04348-4 (hardcover), 1-107-63858-5 (paperback), 1-107-33857-3 (e-book)},
	Journal = {arXiv preprint arXiv:1212.4799},
	Pages = {1--51},
