@techreport{Foushee2016b,
	Abstract = {Children may learn new words better from child-directed speech early on in life in part because it is less complex, which serves to maintain their attention, a necessary precursor to learning. Previous work has suggested that speech to children contains more words they are likely to know than does speech between adults. In this study, we take advantage of longitudinal corpora spanning approximately one year in the development of 12 children, a corpus of diverse, informal adult conversations, and independent ratings of semantic variables to explore a) qualitative changes in speech to children as they mature, b) systematic differences from speech to adults, and c) qualitative variation among adult speakers to both audiences. We find that words in speech to children are reliably more concrete than words in speech to adults, and that child-directed speech becomes less concrete with the age of the child between 20 and 36 months. The same is not true for the valence, or positivity, of words. Regardless of whether caregivers are intentionally scaffolding children's learning or simply trying to communicate effectively, this difference between concrete-ness and valence is consistent with caregivers being sensitive to children's language development, as concrete words are by definition easier to learn without the aid of other language.},
	Author = {Foushee, Ruthe},
	Date-Added = {2019-05-28 12:28:11 -0700},
	Date-Modified = {2019-05-28 12:28:14 -0700},
	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Foushee - 2016 - Mean concreteness as an index of semantic complexity in longitudinal speech to children and adults.pdf:pdf},
	Title = {{Mean concreteness as an index of semantic complexity in longitudinal speech to children and adults}},
	Url = {https://www.viviancook.uk/Words/},
	Year = {2016},
	Bdsk-Url-1 = {https://www.viviancook.uk/Words/}}

@article{Newport1990,
	Abstract = {This paper suggests that there are constraints on learning required to explain the acquisition of language, in particular. maturational constraints. First. empirical evidence for this claim is reviewed. The evidence from several studies of both first and second language acquisition suggests that normal language learning occurs only when exposure to the language begins early in life. With exposure beginning later in life. asymptotic performance in the language declines: the effects over age of first exposure are approximately linear through childhood. with a flattening of the function in adulthood. These outcomes argue that some type of constraints ensuring successful language learning exist early in life, and weaken with increasing maturation. Second. two hypotheses are considered as to the nature of these maturational changes. One hypothesis is that constraints on learning particular to language acquisition undergo maturational decay. A second hypothesis. which is considered in more detail. suggests that language leaming abilities decline because of the expansion of nonlinguistic cognitive abilities. ?? 1990.},
