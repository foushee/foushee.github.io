	Journal = {Journal of Consumer Research},
	Keywords = {Discretionary spending,Experiences and material Goods,Financial constraints,Longevity,Myopia},
	Number = {1},
	Pages = {59--75},
	Title = {{Seeking lasting enjoyment with limited money: Financial constraints increase preference for material goods over experiences}},
