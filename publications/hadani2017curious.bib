	Doi = {10.1111/1460-6984.12126},
	File = {:Users/ruthejane/Box Sync/pdfs/Polisenska{\%}2C{\%}20Chiat{\%}20{\%}26{\%}20Roy{\%}20final{\%}20IJLCD{\%}202014.pdf:pdf},
	Isbn = {1460-6984},
	Issn = {14606984},
	Journal = {International Journal of Language and Communication Disorders},
	Keywords = {assessment,children,cross-linguistic,sentence repetition,span task},
	Number = {1},
	Pages = {106--118},
	Pmid = {25208477},
	Title = {{Sentence repetition: What does the task measure?}},
