	Journal = {Cognitive Science},
	Keywords = {anthropomorphism,conceptual,folk theories,god concepts,religious cognition},
	Pages = {n/a--n/a},
	Title = {{Attributes of God: Conceptual Foundations of a Foundational Belief}},
