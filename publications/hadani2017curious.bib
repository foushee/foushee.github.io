	Issn = {0165-0254},
	Journal = {International Journal of Behavioral Development},
	Keywords = {adhd,attention problems,degree of attention to,detail and,distraction,i,inattention,o,orphans,overactivity,parenting,referring to ease of,typically measured with items},
	Number = {2},
