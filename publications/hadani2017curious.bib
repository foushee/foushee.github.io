	Doi = {10.1177/00030651000480041801},
	File = {:Users/ruthejane/Library/Application Support/Mendeley Desktop/Downloaded/Main - 2000 - The organized categories of infant, child, and adult attachment flexible vs. inflexible attention under attachment-related.pdf:pdf},
	Isbn = {0003-0651, Print},
	Issn = {0003-0651},
